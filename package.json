{
  "name": "voa-admin-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "standardize:win": "prettier **/*.ts --write",
    "standardize:linux": "prettier '**/*.ts' --write",
    "start": "npm run standardize:linux && ng serve --port 5000",
    "build": "npm run standardize:linux && ng build --output-path=./dist",
    "test": "ng test --browsers Chrome --watch=true",
    "test:debug": "ng test --browsers Chrome --sourcemaps=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod --output-path=./dist",
    "build:integration": "cross-conf-env docker build -f Dockerfile.integration . -t $npm_package_config_imageRepo:latest --build-arg CACHEBUST=$(date +%s)",
    "test:prod": "ng test --browsers ChromiumNoSandbox --watch=false",
    "test:prod:e2e": "npm run e2e",
    "test:coverage": "ng test --browsers ChromiumNoSandbox --watch=false --code-coverage",
    "posttest:coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "predeploy": "run-s build:prod test:prod test:prod:e2e",
    "deploy": "firebase deploy"
  },
  "config": {
    "imageRepo": "voa-admin-ui",
    "imageName": "voa-admin-ui",
    "imagePort": "8080"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "angularfire2": "^5.0.0-rc.10",
    "core-js": "^2.5.4",
    "firebase": "^5.0.4",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.2",
    "rxjs": "^6.2.1",
    "ts-enum-util": "^2.0.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cross-conf-env": "^1.1.2",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.3",
    "protractor": "^5.3.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "prettier": {
    "printWidth": 90,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  }
}
